{
  "name": "img-contextual",
  "displayName": "Image To Base64 clipboard",
  "description": "Adds to contextual menu encode to base 64 and set into the clipboard",
  "keywords": [
    "Base64",
    "Images",
    "Encode",
    "Preview"
  ],
  "icon": "images/encoder.png",
  "license": "SEE LICENSE IN LICENSE",
  "publisher": "green-csv",
  "repository": "https://github.com/Asxom/img-contextual",
  "version": "0.2.5",
  "engines": {
    "vscode": "^1.55.0"
  },
  "galleryBanner": {
    "color": "#2C3E50",
    "theme": "dark"
  },
  "categories": [
    "Other",
    "Formatters"
  ],
  "activationEvents": [
    "onCommand:img-contextual.encodeToB64",
    "onCommand:img-contextual.encodeTextToB64",
    "onCommand:img-contextual.decodeB64ToAscii",
    "onCommand:img-contextual.previewB64"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "img-contextual.encodeToB64",
        "title": "Encode: to base64",
        "category": "Encode"
      },
      {
        "command": "img-contextual.encodeTextToB64",
        "title": "Text to base64",
        "category": "Encode"
      },
      {
        "command": "img-contextual.decodeB64ToAscii",
        "title": " Base64 to Ascii",
        "category": "Encode"
      },
      {
        "command": "img-contextual.previewB64",
        "title": "Preview base64 image",
        "category": "Encode"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "img-contextual.encodeToB64",
          "when": "false"
        }
      ],
      "explorer/context": [
        {
          "command": "img-contextual.encodeToB64",
          "when": "resourceLangId == encodingImgTargetFile && !listMultiSelection"
        },
        {
          "command": "img-contextual.encodeTextToB64",
          "when": "false"
        },
        {
          "command": "img-contextual.decodeB64ToAscii",
          "when": "false"
        },
        {
          "command": "img-contextual.previewB64",
          "when": "false"
        }
      ],
      "editor/context": [
        {
          "command": "img-contextual.encodeTextToB64",
          "when": "editorTextFocus",
          "group": "Encode"
        },
        {
          "command": "img-contextual.decodeB64ToAscii",
          "when": "editorTextFocus",
          "group": "Encode"
        },
        {
          "command": "img-contextual.previewB64",
          "when": "editorTextFocus",
          "group": "Encode"
        }
      ]
    },
    "languages": [
      {
        "id": "encodingImgTargetFile",
        "extensions": [
          ".png",
          ".jpg",
          ".jpeg",
          ".gif",
          ".webp",
          ".ico"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "file-type": "14.6.1",
    "jshint": "^2.13.1",
    "vscode-nls-dev": "^4.0.0"
  }
}
